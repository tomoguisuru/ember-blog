{
  "name": "ember-blog",
  "version": "1.0.0",
  "description": "Basic demo of a blog app",
  "license": "MIT",
  "author": "Brandon Drake",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "git@github.com:tomoguisuru/ember-blog.git",
  "scripts": {
    "build": "ember build",
    "build:production": "ember build --environment production",
    "start": "ember server",
    "serve": "ember s --live-reload=false",
    "reset": "yarn unlink && rm -rf node_modules bower_components dist tmp yarn.lock",
    "prep": "yarn install && yarn link",
    "test": "ember test",
    "link:all": "yarn link ember-material"
  },
  "dependencies": {
    "ember-material": "tomoguisuru/ember-material"
  },
  "devDependencies": {
    "broccoli-asset-rev": "^2.4.5",
    "ember-ajax": "^3.0.0",
    "ember-cli": "~2.14.2",
    "ember-cli-app-version": "^3.0.0",
    "ember-cli-babel": "^6.3.0",
    "ember-cli-dependency-checker": "^1.3.0",
    "ember-cli-eslint": "^3.0.0",
    "ember-cli-htmlbars": "^2.0.1",
    "ember-cli-htmlbars-inline-precompile": "^0.4.3",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-mirage": "^0.3.4",
    "ember-cli-moment-shim": "^3.5.0",
    "ember-cli-qunit": "^4.0.0",
    "ember-cli-sass": "^7.0.0",
    "ember-cli-shims": "^1.1.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^1.2.0",
    "ember-data": "~2.14.9",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.0.0",
    "ember-moment": "^7.4.1",
    "ember-page-title": "^3.2.2",
    "ember-resolver": "^4.0.0",
    "ember-source": "~2.14.1",
    "ember-toastr": "^1.7.0",
    "ember-web-app": "^2.0.0",
    "ember-welcome-page": "^3.0.0",
    "ember-wormhole": "^0.5.2",
    "liquid-fire": "^0.28.0",
    "loader.js": "^4.2.3",
    "toastr": "^2.1.2"
  },
  "engines": {
    "node": "^4.5 || 6.* || >= 7.*"
  },
  "private": true
}
